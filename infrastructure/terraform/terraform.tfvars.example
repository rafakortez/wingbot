# Copiar para terraform.tfvars e preencher os valores reais
# NUNCA commitar o terraform.tfvars

# Token da API do DigitalOcean
# Gerar em: https://cloud.digitalocean.com/account/api/tokens
do_token = "dop_v1_XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"

# Conteúdo da chave pública (cat ~/.ssh/nova_kygen.pub)
ssh_public_key = "ssh-ed25519 AAAA..."

# Porta SSH customizada (qualquer porta > 1024, ex: 2277)
ssh_port = 2277
